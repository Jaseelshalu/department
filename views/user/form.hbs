            function changeImage(className) {
                const radioButtons = document.querySelectorAll('.' + className);

                radioButtons.forEach((radioButton) => {
                    const image = radioButton.parentNode.querySelector('.imageRadio');
                    if (radioButton.checked) {
                        image.src = '/images/unlocked.png';
                    } else {
                        image.src = '/images/lock.png';
                    }
                });
            }

            // Call the function when the radio buttons are clicked
            document.addEventListener('click', function (event) {
                const target = event.target;
                if (target.classList.contains('radioImage')) {
                    changeImage('radioImage');
                }
            });

            function validateForm() {
                var radioButtons = document.getElementsByName('Program');
                var isChecked = false;

                for (var i = 0; i < radioButtons.length; i++) {
                    if (radioButtons[i].checked) {
                        isChecked = true;
                        break;
                    }
                }

                if (!isChecked) {
                    alert('Please select an option before submitting.');
                    return false; // Prevent form submission
                }

                return true; // Allow form submission
            }

            function hideParagraph() {
                var paragraph = document.getElementById("hideMe");
                paragraph.style.display = "none";
            }

